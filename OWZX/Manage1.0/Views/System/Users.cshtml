@{
    ViewBag.Title = "用户管理";
    var roelslist = (List<CloudSalesEntity.Manage.M_Role>)ViewBag.Roles;
}

@section scripts{
    <script type="text/javascript">
        seajs.use(["scripts/system/users"], function (obj) {
            obj.init('@Html.ToJSONString(roelslist)');
        });
    </script>
}
<div class="header-box">
    <span class="header-title left">用户管理</span>
    <div class="btn-add right " id="createModel">新建用户</div>
</div>
<div class="content-body mTop20">
    <table class="table-list">
        <tr class="tr-header">
            <td class="tLeft">管理员名称</td>
            <td class="tLeft">角色</td>
            <td class="tLeft">描述</td>
            <td class="width150">创建时间</td>
            <td class="width150">创建人</td>
            <td class="width80">操作</td>
        </tr>
    </table>
</div>
<ul class="dropdown-ul hide" style="width:65px;">
    <li class=" " id="updateObject">编辑</li>
    <li class=" " id="deleteObject">删除</li>
</ul>
